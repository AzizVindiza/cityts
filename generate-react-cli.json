{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "sass",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx",
        "style": "templates/component/TemplateName.module.sass",
        "test": "templates/component/TemplateName.test.tsx",
        "lazy": "templates/component/TemplateName.lazy.tsx"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": true
    },
    "page": {
      "customTemplates": {
        "component": "templates/page/TemplateName.tsx",
        "test": "templates/page/TemplateName.test.tsx",
        "lazy": "templates/page/TemplateName.lazy.tsx"
      },
      "path": "src/pages",
      "withStyle": false,
      "withTest": true,
      "withStory": false,
      "withLazy": true
    },
    "section": {
      "customTemplates": {
        "component": "templates/section/TemplateName.tsx",
        "style": "templates/section/TemplateName.module.sass",
        "test": "templates/section/TemplateName.test.tsx",
        "lazy": "templates/section/TemplateName.lazy.tsx"
      },
      "path": "src/pages/sections",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": true
    }
  }
}